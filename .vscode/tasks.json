  
{
	// See https://go.microsoft.com/fwlink/?LinkId=733558 
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "print",
			"command": "gcc",
			"args": [
				"-g",				
				"-I",
				"${workspaceFolder}/includes/",
				"${workspaceFolder}/libftprintf.a",
				"${workspaceFolder}/fl_main.c",
				"-o",
				"${workspaceFolder}/ft_printf"
			],
			"options": {
				"cwd": "/usr/bin"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	]
}

